{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "globalReturn": true
    }
  },
  "rules": {
    "no-console": 1,
    "no-var": 2,
    "prefer-const": 1,
    "eqeqeq": 2,
    "react/jsx-key": 2,
    "camelcase": 2,
    "no-unneeded-ternary": 2,
    "array-callback-return": 2,
    "react/self-closing-comp": 2,
    "react/prefer-es6-class": 2,
    "react/jsx-handler-names": 2,
    "react/no-unused-state": 2,
    "react/no-unused-prop-types": 2
  }
}